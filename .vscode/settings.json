{
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "git.enableSmartCommit": true,
  "files.exclude": {
    "**/node_modules/": true,
    "yarn.lock": true,
    "package-lock.json": true
  },
  "git.autofetch": true,
  "prettier.eslintIntegration": true,
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "tslint.autoFixOnSave": true,
  "prettier.trailingComma": "all",
  "cSpell.words": [
    "Dhayanandhan",
    "Workspaces",
    "antialiased",
    "bconnect",
    "bwlines",
    "captcha",
    "cellpadding",
    "cellspacing",
    "copyfiles",
    "dhayaec",
    "ecmascript",
    "fastify",
    "flushall",
    "gitignore",
    "gmail",
    "graphiql",
    "lerna",
    "lpush",
    "lrange",
    "lspace",
    "mariadb",
    "nbsp",
    "nohoist",
    "ormconfig",
    "preheader",
    "proviced",
    "repo",
    "resave",
    "rspace",
    "slugify",
    "typeorm",
    "varchar",
    "precommit"
  ],
  "editor.emptySelectionClipboard": false
}
