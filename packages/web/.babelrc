{
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "displayName": true
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "@babel/preset-typescript",
        "next/babel",
        "@zeit/next-typescript/babel"
      ]
    },
    "production": {
      "presets": [
        "@babel/preset-typescript",
        "next/babel",
        "@zeit/next-typescript/babel"
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-typescript",
          "next/babel",
          { "preset-env": { "modules": "commonjs" } }
        ],
        "@zeit/next-typescript/babel"
      ]
    }
  }
}
